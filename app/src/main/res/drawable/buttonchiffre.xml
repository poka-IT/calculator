<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <corners
                android:radius="0dp"
                />
            <solid
                android:color="#848484"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="100dp"
                android:height="100dp"
                />
            <stroke
                android:width="2dp"
                android:color="#323232"
                />
        </shape>
    </item>
    <item>
        <shape>
            <corners
                android:radius="0dp"
                />
            <solid
                android:color="#464646"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="100dp"
                android:height="100dp"
                />
            <stroke
                android:width="2dp"
                android:color="#323232"
                />
        </shape>
    </item>
</selector>